package com.Session5;

public class ExceArray {
	public static void main(String[] args) {
		
			int a[]=new int[3];
			int x=10;
			try{
				for(int i=0;i<4;i++){
				a[i]=x;
				x=x+10;
				}
			}
				catch (Exception e)
				{
					System.out.println(e);
					for(int num:a)
						System.out.println(num);
				}
			}
		
	}


