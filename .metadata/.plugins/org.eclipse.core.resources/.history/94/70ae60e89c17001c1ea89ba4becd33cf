package Learncoding;
import java.util.*;
public class ArrList {
	public static void main(String[] args) {
		ArrayList<Integer> li=new ArrayList<Integer>();
		li.add(5);
		li.add(63);
		li.add(8);
		li.add(10);
		li.add(11);
		li.add(12);
		li.add(5, 19);
		System.out.println(li);
		
		li.remove(6);
	}

}
